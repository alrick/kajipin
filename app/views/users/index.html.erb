<% content_for :page_title do %>Searching User<% end %>

<div class="page-header">
  <h1>Searching User <small><%= page_entries_info @users %> results for "<%= @q %>"</small></h1>
</div>

<div id="users">
  <%= render(:partial => "shared/user_box", :collection => @users, :as => :mate) || content_tag("p", "No user found.", class: "lead") %>
</div>

<%= paginate @users %>