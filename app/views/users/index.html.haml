= content_for :page_title do
	Searching User

.page-header
	%h1
		Searching User
		%small
			= page_entries_info @users
			results for
			= '"' + @q + '"'
#users
	= render(:partial => "shared/user_box", :collection => @users, :as => :mate) || content_tag("p", "No user found.", :class => "lead")
= paginate @users